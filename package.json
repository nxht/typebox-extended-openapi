{
  "name": "@nxht/typebox-extended-openapi",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/nxht/typebox-extended-openapi"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "types": "./src/index.ts",
  "exports": {
    "bun": "./src/index.ts",
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "scripts": {
    "build:esm": "tsc -p tsconfig.build.json --outDir dist/esm -m esnext",
    "build:cjs": "tsc -p tsconfig.build-cjs.json",
    "build": "rm -rf dist/ && bun run build:esm && bun run build:cjs",
    "trace": "bunx tsc --noEmit --generateTrace trace && bunx analyze-trace trace --force-millis 100",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.33.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/bun": "latest",
    "typescript": "^5.5.4"
  }
}
